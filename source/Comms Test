import serial
import struct
import time
Arduino=serial.Serial('/dev/ttyACM0', 9600)
while True:
    for i in range(0,4):
        angle=5*i #0,7.5,15,22.5,30
        for servo in range (0,5):
            Arduino.write(struct.pack('<B', angle)) # sends float angle as a string
            response=Arduino.readline()
            time.sleep(0.1)
            print(int(response))
